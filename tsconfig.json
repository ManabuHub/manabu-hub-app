{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "es6",
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "typeRoots": ["../src/types"]
  },
  "includes": ["babel.config.cjs"],
  "exclude": ["node_modules"]
}
